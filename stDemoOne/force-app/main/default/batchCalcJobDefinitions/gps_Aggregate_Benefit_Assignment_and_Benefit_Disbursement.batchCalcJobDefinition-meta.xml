    <label>Aggregate Benefit Assignment and Benefit Disbursement</label>
    <parameters>
        <dataType>Date</dataType>
        <description>Today&apos;s Date</description>
        <isMultiValue>false</isMultiValue>
        <label>Current Date</label>
        <name>Current_Date</name>
    </parameters>
